<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE tiles-definitions PUBLIC
       "-//Apache Software Foundation//DTD Tiles Configuration 2.0//EN"
       "http://tiles.apache.org/dtds/tiles-config_2_0.dtd">

<tiles-definitions>

	<!-- Default Main Template -->
	<definition name="baseTemplate" template="/WEB-INF/templates/baseTemplate.jsp">
		<put-attribute name="head" value="/WEB-INF/templates/head.jsp" />
		<put-attribute name="header" value="/WEB-INF/templates/header.jsp" />
		<put-attribute name="nav" value="/WEB-INF/templates/nav.jsp" />
		<put-attribute name="roof" value="/WEB-INF/templates/roof.jsp" />
		<put-attribute name="footer" value="/WEB-INF/templates/footer.jsp" />
		<put-attribute name="menu" value="/WEB-INF/templates/menu.jsp" />
		<put-attribute name="body" value="/WEB-INF/templates/blank.jsp" />
	</definition>
	
	
	
 	<definition name="index" extends="baseTemplate">
   		<put-attribute name="body"   value="/WEB-INF/jsp/index.jsp"/>   
   		<put-attribute name="menu"   value="/WEB-INF/jsp/menu.jsp"/>     
 	</definition>
 	
 	<definition name="auth" extends="baseTemplate">
   		<put-attribute name="body"   value="/WEB-INF/jsp/auth/index.jsp"/>     
 	</definition>
 	
 	 <definition name="admin" extends="baseTemplate">
   		<put-attribute name="body"   value="/WEB-INF/jsp/admin/index.jsp"/>     
 	</definition>
 	
  	<definition name="Default" extends="baseTemplate">
   		<put-attribute name="body"   value="/WEB-INF/jsp/router.jsp"/>
   		<put-attribute name="menu"   value="/WEB-INF/jsp/menu.jsp"/>          
 	</definition> 	
 	<definition name="custom" extends="baseTemplate">
   		<put-attribute name="body"   value="/WEB-INF/jsp/custom.jsp"/>
   		<put-attribute name="menu"   value="/WEB-INF/jsp/menu.jsp"/>          
 	</definition> 	
  	
</tiles-definitions>