<project name="SetupProtogen" default="setup_protogen">

	<property environment="env" />


	<target name="setup_protogen">
		<mkdir dir="${user.home}/.ant" />
		<mkdir dir="${user.home}/.ant/lib" />
		<copy todir="${user.home}/.ant/lib" failonerror="false" overwrite="false" verbose="true" >
			<fileset  file="required_jars/ant-contrib-1.0b3.jar" />
			<fileset  file="required_jars/ivy-2.1.0.jar" />
		</copy>
		<echo message="Setup complete."/>
	</target>
	
</project>