*.css  = svn:mime-type=text/css; svn:eol-style=native; svn:keywords=Date Author Id Revision HeadURL;
*.xhtml = svn:mime-type=application/xhtml+xml; svn:eol-style=native; svn:keywords=Date Author Id Revision HeadURL;

# JavaScript Source files
*.js   = svn:mime-type=text/javascript; svn:eol-style=native; svn:keywords=Date Author Id Revision HeadURL;

# Text files
*.txt  = svn:mime-type=text/plain; svn:eol-style=native;
*.dat  = svn:mime-type=text/plain; svn:eol-style=native;
*.conf = svn:mime-type=text/plain; svn:eol-style=native;

# XML files
*.xml  = svn:mime-type=text/xml; svn:eol-style=native; svn:keywords=Date Author Id Revision HeadURL;
